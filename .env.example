# CI/CD Dashboard Configuration

# Set to true for local development without cluster access
MOCK_MODE=true

# Jenkins Configuration
JENKINS_URL=http://jenkins.jenkins.svc.cluster.local:8080
JENKINS_USER=
JENKINS_TOKEN=
JENKINS_JOB_NAME=simple-flask-ci

# ArgoCD Configuration
ARGOCD_URL=https://argocd.argocd.svc.cluster.local
ARGOCD_TOKEN=

# Kubernetes Configuration
K8S_NAMESPACE=web-app

# Flask Configuration
PORT=5000
FLASK_DEBUG=true

# GitHub Repository URLs
GITHUB_APP_REPO=https://github.com/your-username/your-app-repo
GITHUB_INFRA_REPO=https://github.com/your-username/your-infra-repo
